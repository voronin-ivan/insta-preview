{
    "compilerOptions": {
        "module": "esnext",
        "target": "es5",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "lib": ["es6", "dom"],
        "jsx": "react",
        "resolveJsonModule": true
    },
}
